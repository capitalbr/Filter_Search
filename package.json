{
  "name": "react-express-project",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "PORT=3001 node ./bin/www",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run start\" \"npm run client\"",
    "sass": "sass --watch client/src/styles/input.scss:client/src/styles/output.css",
    "devs": "concurrently \"npm run dev\" \"npm run sass\""
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "concurrently": "^4.1.2",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "mongoose": "^5.7.5",
    "morgan": "~1.9.1"
  }
}
